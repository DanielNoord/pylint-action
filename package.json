{
  "name": "pylint-action",
  "version": "0.0.1",
  "description": "GitHub Action that runs pylint over your code.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "test": "jest --runInBand --coverage",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielNoord/pylint-action"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Linting",
    "Python",
    "Pylint"
  ],
  "prettier": {
    "printWidth": 88,
    "tabWidth": 2
  },
  "author": "DaniÃ«l van Noord",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanielNoord/pylint-action/issues"
  },
  "homepage": "https://github.com/DanielNoord/pylint-action",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.0.0",
    "jest": "^29.4.1"
  }
}
